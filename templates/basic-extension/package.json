{
    "name": "basic-extension-template",
    "displayName": "My Extension",
    "description": "Description of what this extension does",
    "version": "0.1.0",
    "publisher": "fabioc-aloha",
    "engines": {
        "vscode": "^1.109.0"
    },
    "categories": [
        "Other"
    ],
    "keywords": [],
    "activationEvents": [
        "onCommand:myExtension.helloWorld"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "myExtension.helloWorld",
                "title": "My Extension: Hello World"
            }
        ]
    },
    "scripts": {
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "package": "npx @vscode/vsce package"
    },
    "devDependencies": {
        "@types/vscode": "^1.109.0",
        "@types/node": "^20.0.0",
        "typescript": "^5.7.0"
    }
}
