{
    "name": "webview-extension-template",
    "displayName": "My Webview Extension",
    "description": "Extension with a Webview panel",
    "version": "0.1.0",
    "publisher": "fabioc-aloha",
    "engines": {
        "vscode": "^1.109.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:myWebview.open"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "myWebview.open",
                "title": "My Webview: Open Panel"
            }
        ]
    },
    "scripts": {
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "package": "npx @vscode/vsce package"
    },
    "devDependencies": {
        "@types/vscode": "^1.109.0",
        "@types/node": "^20.0.0",
        "typescript": "^5.7.0"
    }
}
